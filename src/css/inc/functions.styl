placeholder(color)
  &::-webkit-input-placeholder
    color: color

  &::-moz-placeholder
    color: color
    opacity: 1

  &:-ms-input-placeholder
    color: color

webkitOverflowFix()
  -webkit-mask-image: -webkit-radial-gradient(white, black)

visibility-hidden()
	position absolute
	overflow hidden
	clip rect(0 0 0 0)
